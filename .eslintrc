{ 
  "env": {
    "browser": true
},
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier","prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx"] }
    ],
    "linebreak-style": 0,
    "no-nested-ternary": 0,
    "react/prop-types": 0,
    "consistent-return": 0,
    "react/jsx-fragments": ["warn", "element"]
  }
}